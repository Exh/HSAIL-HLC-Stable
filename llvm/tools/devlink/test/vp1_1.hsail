version 1:0:small;

function &foo()();
function &foo2()();

extern global_s32 &gVar;
extern global_s32 &gVar2;

extern global_s32 &undefVar;

kernel &main_kernel()
{
	ld_global_s32 $s1,[&gVar];
	ld_global_s32 $s1,[&gVar2];

	ld_global_s32 $s1,[&undefVar];
	{
		call &foo;
	}
	{
		call &foo2;
	}	
};



